{
  "name": "backend",
  "version": "0.0.1",
  "description": "TypeScript + Express + Routing Controllers API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "dotenv -e .env.production.local node dist/server.js",
    "dev": "dotenv -e .env.development.local nodemon",
    "dev:test": "dotenv -e .env.test.local nodemon",
    "prisma": "dotenv -e .env.development.local -- prisma",
    "prisma:prod": "dotenv -e .env.production.local prisma",
    "prisma:migrate": "dotenv -e .env.development.local prisma migrate deploy",
    "prisma:seed": "dotenv -e .env.development.local ts-node prisma/seed/seed.ts",
    "prisma:generate": "dotenv -e .env.development.local prisma generate",
    "prisma:new-migrate": "dotenv -e .env.development.local yarn prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.production.local prisma migrate deploy",
    "prisma:seed:prod": "dotenv -e .env.production.local ts-node prisma/seed/seed.ts",
    "prisma:generate:prod": "dotenv -e .env.production.local prisma generate",
    "prisma:start": "yarn prisma:generate && yarn prisma:migrate && yarn prisma:seed:texts",
    "prisma:start:prod": "yarn prisma:generate:prod && yarn prisma:migrate:prod && yarn prisma:seed:texts:prod",
    "generate:contracts": "cross-env NODE_ENV=development ts-node ./src/swagger-typescript-api.ts",
    "build": "tsc && tsc-alias",
    "build:swc": "swc src -d dist --source-maps --copy-files",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "axios": "^1.12.0",
    "bcrypt": "5.1.1",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-jsonschema": "^5.0.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "envalid": "^8.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "memorystore": "^1.6.7",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "qs": "^6.14.0",
    "ra-data-simple-prisma": "^5.3.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.2",
    "routing-controllers-openapi": "^4.1.0",
    "sanitize-html": "^2.15.0",
    "session-file-store": "^1.5.0",
    "sharp": "^0.33.5",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.15",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.16",
    "@types/passport": "^1.0.17",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "node-config": "^0.0.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prisma": "^5.22.0",
    "supertest": "^6.3.4",
    "swagger-typescript-api": "^13.0.28",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
